<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
  <div class="jumbotron">
    <h1>Jackson Johnson</h1>
    <h3>Mission - Create High Quality Scripts</h3>
    <p><%= tagline %></p>

    <ul>
        <% mascots.forEach(function(mascot) { %>
          <li>
            <strong><%= mascot.name %></strong>
            is an <%= mascot.description %>,
            TRiBot <%= mascot.released %>
          </li>
        <% }); %>
      </ul>

  </div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>